<colspec colnum="3" colname="c3"/>
<colspec colnum="4" colname="c4"/>
<thead>
<row valign="top">
<entry valign="top" align="center">Corrimiento Doppler<newline/><m:math><m:mrow><m:msub><m:mi>f</m:mi><m:mtext>o</m:mtext></m:msub><m:mo>=</m:mo><m:msub><m:mi>f</m:mi><m:mtext>s</m:mtext></m:msub><m:mrow><m:mo>(</m:mo><m:mrow><m:mfrac><m:mrow><m:mi>v</m:mi><m:mo>±</m:mo><m:msub><m:mi>v</m:mi><m:mtext>o</m:mtext></m:msub></m:mrow><m:mrow><m:mi>v</m:mi><m:mo>∓</m:mo><m:msub><m:mi>v</m:mi><m:mtext>s</m:mtext></m:msub></m:mrow></m:mfrac></m:mrow><m:mo>)</m:mo></m:mrow></m:mrow></m:math></entry>
